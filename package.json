{
  "name": "@kronos-integration/service-koa",
  "version": "0.0.0-semantic-release",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/service-koa.mjs",
  "module": "src/service-koa.mjs",
  "description": "koa backed http server",
  "keywords": [
    "kronos-service"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/service-koa.mjs --section=API",
    "lint": "documentation lint src/service-koa.mjs",
    "posttest": "markdown-doctest",
    "test": "ava"
  },
  "dependencies": {
    "@kronos-integration/service": "^7.1.23",
    "bufferutil": "^4.0.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.11.0",
    "model-attributes": "^4.1.3",
    "multi-path-matcher": "^2.0.4",
    "utf-8-validate": "^5.0.2",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "c8": "^6.0.1",
    "documentation": "^12.1.4",
    "esm": "^3.2.25",
    "got": "^10.0.1",
    "koa-route": "^3.2.0",
    "markdown-doctest": "^1.0.0",
    "network-address": "^1.1.2",
    "semantic-release": "^15.13.31"
  },
  "engines": {
    "node": ">=12.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service-koa.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service-koa/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service-koa#readme",
  "ava": {
    "files": [
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  },
  "sideEffects": false
}
